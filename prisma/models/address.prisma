model City {
  id        Int      @id @default(autoincrement())
  name      String
  createdBy Int?
  createdAt DateTime @default(now())

  users     UserDetails[]
  libraries LibraryLocation[]
}

model State {
  id        Int      @id @default(autoincrement())
  name      String
  createdBy Int?
  createdAt DateTime @default(now())

  users     UserDetails[]
  libraries LibraryLocation[]
}

model Country {
  id        Int      @id @default(autoincrement())
  name      String
  createdBy Int?
  createdAt DateTime @default(now())

  users     UserDetails[]
  libraries LibraryLocation[]
}
